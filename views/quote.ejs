<%- include("partials/home_header") %>

<body>
    <section id="main" class="py-0">
        <div class="container">
          <header class="d-flex flex-wrap justify-content-md-end  justify-content-center py-3 mb-4 border-bottom">
            <ul class="nav nav-pills align-items-center">
              <li class="nav-item"><a href="/logout" class="nav-link"><button type="button" id="login-btn" class="btn btn-secondary">Logout</button></a></li>
              <img src="/assets/ChilloutLogo_2.png" alt="ChillOut Logo" class="px-3" height="40px">
            </ul>
          </header>
        </div>
    
        <div class="px-4 py-5 mb-5 text-center main-cont">
          <h1 class="display-5 main-text fw-normal"><%= quote %></h1>
          <div class="col-lg-6 mx-5">
            <p class="lead mb-4 main-text fw-normal text-end"> - <%= quoteAuthor %> </p>
            <div class="gap-2 d-sm-flex justify-content-sm-center">
              <a href="/breathe" class="nav-link"><button type="button" id="begin-btn" class="btn btn-primary btn-lg px-5 gap-3">Take Another Breather</button></a>
            </div>
          </div>
        </div>
      </section>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
</body>
</html>