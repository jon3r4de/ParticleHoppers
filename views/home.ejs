    <div class="px-4 py-5 mb-5 text-center main-cont">
      <h1 class="display-5 main-text fw-normal mainText"><%= mainText %></h1>
      <div class="col-lg-6 mx-5">
        <p class="lead mb-4 main-text fw-normal subText"><%= subText %> </p>
      </div>
    </div>
  </section>

  <section id="mission">
    <div class="px-4 py-5 mb-5 text-center mission-cont">
